{
  "metadata": {
    "title": "HUD Fair Housing Act Disability Discrimination Complaints",
    "description": "Fair housing complaint data from the U.S. Department of Housing and Urban Development (HUD) Office of Fair Housing and Equal Opportunity (FHEO). Disability is the most frequently cited basis for fair housing complaints, accounting for over 55% of all complaints filed with HUD. Data compiled from HUD FHEO Annual Reports to Congress and National Fair Housing Alliance (NFHA) Fair Housing Trends Reports.",
    "sources": [
      {
        "name": "HUD FHEO Annual Reports to Congress",
        "url": "https://www.hud.gov/program_offices/fair_housing_equal_opp/aboutfheo/annualreport",
        "description": "Official annual reports from HUD's Office of Fair Housing and Equal Opportunity containing complaint filing statistics, basis breakdowns, resolution outcomes, and state-level data.",
        "coverage": "FY2008-FY2022",
        "data_type": "aggregate_statistics"
      },
      {
        "name": "National Fair Housing Alliance (NFHA) Fair Housing Trends Reports",
        "url": "https://nationalfairhousing.org/reports/",
        "description": "Annual reports compiling fair housing complaint data from all sources: HUD, FHAP agencies, and private fair housing organizations.",
        "coverage": "2008-2022",
        "data_type": "aggregate_statistics"
      },
      {
        "name": "data.gov FHEO Filed Title VIII Cases",
        "url": "https://catalog.data.gov/dataset/fheo-filed-cases",
        "resource_url": "https://inventory.data.gov/dataset/c592e01e-3ce8-4fb7-b112-8b5819fd2072/resource/94b8f073-7d71-4519-8a2b-2cec554e3dcd/download/fheo-filed-cases.xlsx",
        "description": "FHEO Filed Title VIII Cases dataset from data.gov (Excel format)",
        "format": "XLSX",
        "data_type": "case_level_data"
      },
      {
        "name": "Fair Housing Act, 42 U.S.C. 3601-3619",
        "url": "https://www.justice.gov/crt/fair-housing-act-1",
        "description": "Federal law prohibiting discrimination in housing. Disability protections added by Fair Housing Amendments Act of 1988."
      }
    ],
    "legal_framework": {
      "statute": "Fair Housing Act (Title VIII of the Civil Rights Act of 1968)",
      "disability_amendment": "Fair Housing Amendments Act of 1988",
      "protected_classes": [
        "Race",
        "Color",
        "National Origin",
        "Religion",
        "Sex",
        "Familial Status",
        "Disability"
      ],
      "disability_protections": [
        "Prohibition of discrimination in sale or rental of housing",
        "Right to reasonable accommodations in rules, policies, practices, or services",
        "Right to reasonable modifications of existing premises",
        "Accessibility requirements for new multifamily housing (4+ units built after March 13, 1991)"
      ],
      "enforcement_agencies": [
        "HUD Office of Fair Housing and Equal Opportunity (FHEO)",
        "State and local Fair Housing Assistance Program (FHAP) agencies",
        "Department of Justice Civil Rights Division",
        "Private fair housing organizations"
      ]
    },
    "data_notes": [
      "HUD complaint counts are fiscal year (Oct 1 - Sep 30), not calendar year",
      "A single complaint can cite multiple protected class bases, so basis percentages sum to >100%",
      "NFHA totals include complaints from HUD, FHAP agencies, AND private organizations",
      "NFHA totals are significantly higher than HUD-only counts",
      "Disability has been the #1 cited basis in HUD complaints since approximately 2005",
      "Estimated disability complaint counts = total_complaints * disability_pct / 100",
      "State-level counts reflect HUD-filed complaints; FHAP states handle more locally"
    ],
    "retrieved_date": "2026-02-13",
    "excel_data_available": true,
    "data_gov_note": "Includes case-level data computed from 130,000+ individual FHEO filed case records downloaded from data.gov (Excel format). This supplements the aggregate statistics from HUD Annual Reports with granular year/state breakdowns."
  },
